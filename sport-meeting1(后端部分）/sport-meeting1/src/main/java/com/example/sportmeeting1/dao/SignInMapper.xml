<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sportmeeting1.dao.SignInMapper">
    <resultMap id="BaseResultMap" type="com.example.sportmeeting1.model.SignIn">
        <id column="order_number" jdbcType="INTEGER" property="orderNumber" />
        <result column="team_id" jdbcType="VARCHAR" property="teamId" />
        <result column="team_name" jdbcType="VARCHAR" property="teamName" />
        <result column="team_password" jdbcType="VARCHAR" property="teamPassword" />
    </resultMap>
    <select id="selectAll" resultMap="BaseResultMap" >
        select
            order_number, team_id, team_name, team_password
        from sign_in
    </select>
    <insert id="insert" parameterType="com.example.sportmeeting1.model.SignIn">
        insert into sign_in (team_name, team_id, team_password
        )
        values (#{teamName,jdbcType=VARCHAR}, #{teamId,jdbcType=VARCHAR}, #{teamPassword,jdbcType=VARCHAR}
               )
    </insert>
</mapper>