<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sportmeeting1.dao.AthleteJoinPreliminaryMapper">
    <resultMap id="BaseResultMap" type="com.example.sportmeeting1.model.AthleteJoin">
        <id column="order_number" jdbcType="INTEGER" property="orderNumber" />
        <result column="project_number" jdbcType="INTEGER" property="projectNumber" />
        <result column="athlete_number" jdbcType="INTEGER" property="athleteNumber" />
        <result column="score" jdbcType="INTEGER" property="score" />
        <result column="group_number" jdbcType="INTEGER" property="groupNumber" />
        <result column="athlete_name" jdbcType="VARCHAR" property="athleteName" />
        <result column="team_id" jdbcType="VARCHAR" property="teamId" />
    </resultMap>
    <insert id="insertAthleteJoinPreliminary" >
        insert into athlete_join_preliminary(team_id,athlete_name,athlete_number,project_number,score)
        values (#{teamId},#{athleteName},#{athleteNumber},#{projectNumber},#{score})
    </insert>
    <select id="selectByProjectNumber" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select * from athlete_join_preliminary where project_number = #{projectNumber}
    </select>
    <select id="selectByProjectNumberAndTeamId" resultMap="BaseResultMap">
        select * from athlete_join_preliminary where project_number = #{projectNumber} and team_id= #{teamId}
    </select>
</mapper>